{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "no-alert": "off",
        "no-restricted-syntax": "off"
    }
}
